<header [ngClass]="isFixedMenu?'fixed-menu':''">
  <div class='container'>
    <div class='mobile_nav'>
      <button class='burger' title='Open and close menu'>
        <span class='mobile_nav__label'>Open and close menu</span>
        <div class='top stripe'></div>
        <div class='middle stripe'></div>
        <div class='bottom stripe'></div>
      </button>
    </div>
    <div class='mobile_menu'>
      <nav>
        <ul>
          <li class='visited'>
            <a [routerLink]="'/dashboard/projects'"> Project </a>
          </li>
          <li>
            <a [routerLink]="'/dashboard/issues-projects'"> Issues </a>
          </li>
          <!-- <li>
            <a href='/forum'>Forum</a>
          </li>
          <li>
            <a href='/blog'>Blog</a>
          </li>
          <li>
            <a href='/contact'>Kontakt</a>
          </li> -->
        </ul>
      </nav>
    </div>
    <div class='logo'>


      <span>
        <app-menu-loading>
          <span>PalTech</span>
        </app-menu-loading>
      </span>

      <span> </span>
    </div>

    <nav class='head_nav'>
      <ul style="float: left;">
        <li [ngClass]=" rla1.isActive?'visited':''">
          <a [routerLink]="'/dashboard/projects'" routerLinkActive #rla1="routerLinkActive"> Project </a>
        </li>
        <li [ngClass]=" rla2.isActive?'visited':''">
          <a [routerLink]="'/dashboard/issues-projects'" routerLinkActive #rla2="routerLinkActive"> Issues </a>
        </li>

        <li [ngClass]=" rla4.isActive?'visited':''">
          <a [routerLink]="'/manager/user/view-report-send'" routerLinkActive #rla4="routerLinkActive"> Report Send </a>
        </li>

        <li [ngClass]=" rla5.isActive?'visited':''">
          <a [routerLink]="'/manager/user/view-report-save'" routerLinkActive #rla5="routerLinkActive"> Report save </a>
        </li>
        <li [ngClass]=" rla6.isActive?'visited':''">
          <a [routerLink]="'/manager/user/card-list/my-card'" routerLinkActive #rla6="routerLinkActive"> Card List </a>
        </li>
        <!-- <li>
          <a href='/forum'>Forum</a>
        </li>
        <li>
          <a href='/blog'>Blog</a>
        </li>
        <li>
          <a href='/contact'>Kontakt</a>
        </li> -->
      </ul>
    </nav>
    <div class='icons'>
      <ng-container *ngIf="userInfo">
        <div class='block'>
